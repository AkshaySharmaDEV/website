<RoutingRules>
  <RoutingRule>
    <Condition>
      <KeyPrefixEquals>ja/</KeyPrefixEquals>
      <HttpErrorCodeReturnedEquals>403</HttpErrorCodeReturnedEquals>
    </Condition>
    <Redirect>
      <HostName>www.dskobe.org</HostName>
      <ReplaceKeyWith>ja/error/</ReplaceKeyWith>
    </Redirect>
  </RoutingRule>
  <RoutingRule>
    <Condition>
      <HttpErrorCodeReturnedEquals>403</HttpErrorCodeReturnedEquals>
    </Condition>
    <Redirect>
      <HostName>www.dskobe.org</HostName>
      <ReplaceKeyWith>en/error/</ReplaceKeyWith>
    </Redirect>
  </RoutingRule>
</RoutingRules>
